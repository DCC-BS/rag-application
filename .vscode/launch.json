{
  "configurations": [
    {
      "args": [
        "main:app",
        "--reload",
        "--port",
        "8080"
      ],
      "cwd": "${workspaceFolder}/api",
      "env": {
        "PYTHONPATH": "${workspaceFolder}${pathSeparator}api${env:PYTHONPATH}"
      },
      "jinja": true,
      "module": "uvicorn",
      "name": "Python Debugger: FastAPI",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "run",
        "src/rag/ui/app.py"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}${pathSeparator}api${env:PYTHONPATH}"
      },
      "justMyCode": false,
      "module": "streamlit",
      "name": "Python Debugger: Streamlit",
      "request": "launch",
      "type": "debugpy"
    },
    {
      "args": [
        "run",
        "src/rag/ui/app.py"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}${pathSeparator}api${env:PYTHONPATH}"
      },
      "justMyCode": false,
      "module": "streamlit",
      "name": "Streamlit",
      "request": "launch",
      "type": "python"
    }
  ],
  "version": "0.2.0"
}
